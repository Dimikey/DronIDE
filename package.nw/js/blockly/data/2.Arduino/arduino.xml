<category name="Порты ввода вывода" colour="170">
    <block type="arduino_digital_write"></block>
    <block type="arduino_digital_read"></block>
    
    <block type="arduino_analog_write">
        <value name="NUM">
            <block type="math_number">
                <field name="NUM">0</field>
            </block>
        </value>
    </block>

